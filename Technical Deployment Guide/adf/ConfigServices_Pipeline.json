{
    "name": "ConfigServices_Pipeline",
    "properties": {
        "description": "Pipeline to populate the services config",
        "activities": [
            {
                "type": "Copy",
                "typeProperties": {
                    "source": {
                        "type": "SqlSource",
                        "sqlReaderQuery": "select p1.paramValue as deltaX from Parameters p1 where p1.paramName = 'BulkElasticities_deltaX'"
                    },
                    "sink": {
                        "type": "BlobSink",
                        "writeBatchSize": 0,
                        "writeBatchTimeout": "00:00:00"
                    }
                },
                "inputs": [
                    {
                        "name": "solution_parameters_AzureSQLDataset"
                    }
                ],
                "outputs": [
                    {
                        "name": "params_BulkElasticities_AzureBlobDataset"
                    }
                ],
                "policy": {
                    "timeout": "1.00:00:00",
                    "concurrency": 1,
                    "retry": 3
                },
                "scheduler": {
                    "frequency": "Week",
                    "interval": 1
                },
                "name": "Config_BulkElasticities_CopyActivity"
            },
            {
                "type": "Copy",
              "typeProperties": {
                "source": {
                  "type": "SqlSource",
                  "sqlReaderQuery": "select p1.paramValue as WeekJump, p2.paramValue as periodsAhead from Parameters p1, Parameters p2 where p2.paramName = 'BulkForecasts_periodsAhead'  and p1.paramName = 'BulkForecasts_WeekJump"
                },
                "sink": {
                  "type": "BlobSink",
                  "writeBatchSize": 0,
                  "writeBatchTimeout": "00:00:00"
                }
              },
                "inputs": [
                    {
                        "name": "solution_parameters_AzureSQLDataset"
                    }
                ],
                "outputs": [
                    {
                        "name": "params_BulkForecasts_AzureBlobDataset"
                    }
                ],
                "policy": {
                    "timeout": "1.00:00:00",
                    "concurrency": 1,
                    "retry": 3
                },
                "scheduler": {
                    "frequency": "Week",
                    "interval": 1
                },
                "name": "Config_BulkForecasts_CopyActivity"
            }
        ],
        "start": "2017-05-01T00:00:00Z",
        "end": "2017-05-01T00:00:00Z",
        "isPaused": false,
        "hubName": "pricingdemodf_hub",
        "pipelineMode": "OneTime",
        "expirationTime": "3.00:00:00"
    }
}